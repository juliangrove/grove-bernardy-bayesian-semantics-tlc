#     'goodlass-l0y.1d.dat' title "l0 margin. height" with lines, \ # has an asymptote near the maximum of the interval.
#     'goodlass-l0x.1d.dat' title "l0 margin. threshold" with lines, \ # same as prior, not interesting

set terminal svg;
set output 'goodlass.svg';
plot 'goodlass-thres-prior.1d.dat' title "prior threshold" with lines, \
     'goodlass-height-prior.1d.dat' title "prior height" with lines, \
     'goodlass-l1y.1d.dat' title "l1 margin. height" with lines, \
     'goodlass-l1x.1d.dat' title "l1 margin. threshold" with lines;
